{
  "name": "bonustraining",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.4.1",
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "REACT_APP_ENV=development && cp ${REACT_APP_ENV}.env .env && react-scripts start",
    "build": "npm run build",
    "build-dev": "cp development.env .env && react-scripts build",
    "build-test": "cp test.env .env && react-scripts build",
    "build-prod": "cp production.env .env && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "redux-devtools-extension": "^2.13.5"
  }
}
